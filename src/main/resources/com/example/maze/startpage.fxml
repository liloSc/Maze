<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" onKeyReleased="#switchToCharacterSelection" prefHeight="600.0" prefWidth="800.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.maze.ControllerStartPage">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="gameName" text="Name of Game">
      <font>
         <Font name="Retro Gaming" size="47.0" />
      </font></Label>
   <ImageView fitHeight="283.0" fitWidth="338.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@images/Luchu_front_bigger.png" />
      </image>
   </ImageView>
    <Button onKeyPressed="#switchToCharacterSelection" prefHeight="54.0" prefWidth="166.0" style="-fx-background-color: Transparent;" text="press enter" textAlignment="CENTER" textFill="#322eff">
      <font>
         <Font name="Retro Gaming" size="18.0" />
      </font>
   </Button>
</VBox>
