<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane onKeyReleased="#switchToHomeScreen" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.maze.ControllerGameOver">
   <children>
      <Label layoutX="255.0" layoutY="142.0" text="Game Over">
         <font>
            <Font name="Retro Gaming" size="60.0" />
         </font>
      </Label>
      <Label layoutX="302.0" layoutY="260.0" text="Press enter to restart">
         <font>
            <Font name="Retro Gaming" size="20.0" />
         </font>
      </Label>
      <Button layoutX="378.0" layoutY="374.0" mnemonicParsing="false" onAction="#quit" prefHeight="41.0" prefWidth="142.0" style="-fx-background-color: Chocolate;" text="EXIT">
         <font>
            <Font name="Retro Gaming" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
